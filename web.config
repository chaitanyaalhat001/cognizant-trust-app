<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified"/>
    </handlers>
    <httpPlatform processPath="python"
                  arguments="startup_debug.py"
                  stdoutLogEnabled="true"
                  stdoutLogFile="D:\home\LogFiles\Application\python-stdout.log"
                  stderrLogEnabled="true"
                  stderrLogFile="D:\home\LogFiles\Application\python-stderr.log"
                  startupTimeLimit="300"
                  requestTimeout="00:04:00">
      <environmentVariables>
        <environmentVariable name="DJANGO_SETTINGS_MODULE" value="cognizanttrust.settings" />
        <environmentVariable name="PYTHONPATH" value="D:\home\site\wwwroot" />
        <environmentVariable name="PYTHONUNBUFFERED" value="1" />
      </environmentVariables>
    </httpPlatform>
  </system.webServer>
</configuration> 